{
  "expo": {
    "name": "Sorbisch leicht",
    "slug": "sorbisch-leicht",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sorbischleicht",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.sorbischleicht.app",
      "supportsTablet": true
    },
    "android": {
      "package": "de.witaj.sorbischesprache.base",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "permissions": []
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-router",
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "fefc9c88-84e6-4817-b87c-45c8bb50d501"
      }
    },
    "experiments": {
      "reactCompiler": true
    }
  }
}
